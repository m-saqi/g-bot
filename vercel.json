{
  "version": 2,
  "builds": [
    {
      "src": "api/search.py",
      "use": "@vercel/python"
    },
    {
      "src": "templates/**",
      "use": "@vercel/static"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/search.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/templates/$1"
    }
  ],
  "functions": {
    "api/search.py": {
      "maxDuration": 60
    }
  }
}
